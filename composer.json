{
    "name": "bcremer/flexihash",
    "type": "library",
    "description": "Flexihash is a small PHP library which implements consistent hashing",
    "homepage": "https://github.com/bremer/flexihash",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Annesley",
            "email": "paul@annesley.cc",
            "homepage": "http://paul.annesley.cc"
        },
        {
            "name": "Dom Morgan",
            "email": "dom@d3r.com",
            "homepage": "https://d3r.com"
        }
    ],
    "require": {
        "php": ">= 8.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "squizlabs/php_codesniffer": "^3.11"
    },
    "conflict": {
        "bcremer/flexihash": "*"
    },
    "autoload": {
        "psr-4": {
            "Flexihash\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flexihash\\Tests\\": "tests/"
        }
    }
}
